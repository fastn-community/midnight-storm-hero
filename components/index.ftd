-- component stack-desktop:
caption title:
optional body subtitle:
ftd.image-src image:
optional string cta-primary:
optional string cta-primary-url:
optional string cta-secondary:
optional string cta-secondary-url:
optional boolean $mouse-in: false
optional boolean $mouse-enter: false

-- ftd.column:
align-content: center
width: fill-container
spacing.fixed.px: 32

-- ftd.text: $stack-desktop.title
role: $inherited.types.heading-large
color: $inherited.colors.text-strong
text-align: center

-- ftd.text:
role: $inherited.types.copy-regular
color: $inherited.colors.text
text-align: center
width.fixed.percent: 70

$stack-desktop.subtitle

-- ftd.row:
align-self: center
spacing.fixed.px: 16

-- ftd.row:
background.solid: $inherited.colors.cta-primary.base
border-color: $inherited.colors.cta-primary.base
border-width.px: 1
background.solid if { stack-desktop.mouse-in }: $inherited.colors.cta-primary.hover
$on-mouse-enter$: $ftd.set-bool($a = $stack-desktop.mouse-in, v = true)
$on-mouse-leave$: $ftd.set-bool($a = $stack-desktop.mouse-in, v = false)
padding-horizontal.px: 28
padding-vertical.px: 10

-- ftd.text: $stack-desktop.cta-primary
if: { stack-desktop.cta-primary != NULL }
role: $inherited.types.button-large
color: $inherited.colors.background.base
link: $stack-desktop.cta-primary-url

-- end: ftd.row

-- ftd.row:
if: { stack-desktop.cta-secondary != NULL }
border-color: $inherited.colors.shadow
border-width.px: 1
background.solid if { stack-desktop.mouse-enter }: $inherited.colors.cta-primary.hover
$on-mouse-enter$: $ftd.set-bool($a = $stack-desktop.mouse-enter, v = true)
$on-mouse-leave$: $ftd.set-bool($a = $stack-desktop.mouse-enter, v = false)
