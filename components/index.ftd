padding-vertical.px: 10

-- ftd.text: $stack-desktop.cta-secondary
role: $inherited.types.button-large
color: $inherited.colors.shadow
color if { stack-desktop.mouse-enter }: $inherited.colors.background.step-1
link: $stack-desktop.cta-secondary-url

-- end: ftd.row

-- end: ftd.row

-- ftd.image:
src: $stack-desktop.image
width: fill-container
z-index: 5

-- end: ftd.column

-- end: stack-desktop










